{
  "ConnectionStrings": {
    "MongoDb": "mongodb://localhost:27017/?readPreference=primary&appname=IpLookup&directConnection=true&ssl=false",
    "MongoDbName": "IpLookup"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ApiServerKeySettings": {
    "RequireKey": true,
    "QueryName": "key",
    "Key": "PUT_IN_USER_SECRETS_MIN_32_CHARS"
  },
  "Clients": [
    {
      "Name": "ipapi",
      "Site": "ipapi.com",
      "Enabled": true,
      "ApiKey": "<PUT_YOUR_API_KEY_IN_USER_SECRETS>",
      "RateLimitingRules": [
        {
          "Occurrences": 1000,
          "TimeUnit": "1.00:00:00"
        }
      ]
    }
  ]
}
