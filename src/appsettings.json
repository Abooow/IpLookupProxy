{
  "ConnectionStrings": {
    "MongoDb": "mongodb://localhost:27017/?readPreference=primary&appname=IpLookup&directConnection=true&ssl=false",
    "MongoDbName": "IpLookup"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Clients": [
    {
      "Name": "ipapi",
      "ApiKey": "<YOUR_API_KEY>",
      "RateLimitingRules": [
        {
          "Occurrences": 1000,
          "TimeUnit": "1.00:00:00"
        }
      ]
    }
  ]
}
